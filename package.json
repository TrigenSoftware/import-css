{
  "name": "import-css",
  "version": "2.0.0",
  "description": "Load CSS asynchronously without render blocking in the given order.",
  "main": "lib/link-async.js",
  "scripts": {
    "eslint": "eslint --cache 'src/*.js'",
    "compile": "babel ./src -d ./lib",
    "prepublish": "npm run compile"
  },
  "repository": "https://github.com/TrigenSoftware/import-css",
  "author": "dangreen",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-babili": "^0.0.11",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "eslint": "^3.15.0",
    "eslint-config-trigen": "^1.1.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      ["babili", {
        "mangle": {
          "topLevel": true
        }
      }]
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
